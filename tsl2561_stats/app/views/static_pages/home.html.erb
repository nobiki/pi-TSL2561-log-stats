<div class="row">
<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-0 main">
  <h1 class="page-header">PI TSL2561 Status</h1>

  <div class="row placeholders">
    <div class="col-xs-6 col-sm-10">
      <img src="/assets/luxes.jpg" class="img-rounded" alt="Generic placeholder thumbnail">
      <h4>Sample</h4>
      <span class="text-muted">sample data loaded.</span>
    </div>
  </div>

  <h2 class="sub-header"></h2>
  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th>recorded_at</th>
          <th>Lux</th>
        </tr>
      </thead>
      <% Lux.find_each do |i| %>
        <tr>
          <td><%=i.recorded_at%></td>
          <td><%=i.lux%></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
</div>
